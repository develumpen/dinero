<div class="imports">
  <h1><%= @csv_import.account.name %></h1>

  <table class="table">
    <thead>
      <tr>
        <% @csv_headers.each do |header| %>
          <th><%= header %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @csv_rows.each do |columns| %>
        <tr>
          <% columns.each do |column| %>
            <td><%= column %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
