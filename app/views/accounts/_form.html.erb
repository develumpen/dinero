<%= form_with(model: account) do |form| %>
  <%= account.errors.full_messages.join("\n") %>

  <div>
    <%= form.label :name %><br />
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :description %><br />
    <%= form.text_area :description %>
  </div>

  <%= form.submit %>
<% end %>
